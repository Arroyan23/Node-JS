<h1>Data <%= nama %></h1>
<% if (!ambilFiles) { %>
<div class="alert alert-danger" role="alert">
  Tidak Menemukan nama yang anda maksud
  <a href="/data" class="alert-link">&laquo; Kembali Ke menu</a>.
</div>
<% } else { %>
<div class="card" style="width: 18rem">
  <div class="card-body">
    <h5 class="card-title"><%= ambilFiles.nama %></h5>
    <h6 class="card-subtitle mb-2 text-muted">Card subtitle</h6>
    <p class="card-text">
      Nomor Handphone : <%= ambilFiles.nohp %>
      <br />
      Email : <%= ambilFiles.email %>
    </p>
  </div>
</div>
<h1 class="mt-3">Kasus Kasus <%= ambilFiles.nama %></h1>
<% } %>
